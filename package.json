{
  "name": "system",
  "version": "1.0.0",
  "main": "build/main.js",
  "private": true,
  "scripts": {
    "test": "jest --detectOpenHandles",
    "lint": "eslint src --fix",
    "start": "node .",
    "build": "tsc && yarn run tsc:public && yarn run tw",
    "start:dev": "nodemon sandbox/main.ts",
    "start:prod": "yarn run build && dotenv -e sandbox/.env.local -v NODE_ENV=production node .",
    "tw": "postcss public/src/css/tailwind.css -o public/dist/css/style.css",
    "tw:w": "postcss public/src/css/tailwind.css -o public/dist/css/style.css -w",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate:reset": "prisma migrate reset --force",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "tsc:public": "tsc -p public/",
    "tsc:public:w": "tsc -p public/ --watch"
  },
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cli-color": "^2.0.3",
    "compression": "^1.7.4",
    "detect-port": "^1.5.1",
    "ejs": "^3.1.8",
    "env-var": "^7.3.0",
    "express": "^4.18.2",
    "express-list-endpoints": "^6.0.0",
    "express-minify": "^1.0.0",
    "express-minify-html-terser": "^1.0.2",
    "express-mysql-session": "^2.1.8",
    "express-rate-limit": "^6.6.0",
    "express-session": "^1.17.3",
    "express-socket.io-session": "^1.3.5",
    "memory-cache": "^0.2.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.8.0",
    "recursive-readdir": "^2.2.2",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "9.1.6",
    "@prisma/generator-helper": "4.0.0",
    "@types/cli-color": "^2.0.2",
    "@types/compression": "^1.7.2",
    "@types/detect-port": "^1.3.2",
    "@types/express": "^4.17.14",
    "@types/express-list-endpoints": "^6.0.0",
    "@types/express-minify": "^0.1.35",
    "@types/express-mysql-session": "^2.1.3",
    "@types/express-session": "^1.17.5",
    "@types/express-socket.io-session": "^1.3.6",
    "@types/jest": "^29.1.2",
    "@types/memory-cache": "^0.2.2",
    "@types/morgan": "^1.9.3",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11.0",
    "@types/nodemailer": "^6.4.6",
    "@types/recursive-readdir": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.25.0",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-nesting": "^10.2.0",
    "prisma": "^4.5.0",
    "prisma-erd-generator": "^1.2.1",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "yarn": "^1.22.19"
  },
  "engines": {
    "node": ">=16.14.2",
    "npm": ">=8.14.0",
    "yarn": ">=1.22.19"
  },
  "repository": "https://github.com/jmrl23/system.git",
  "contributors": [
    "Jomariel Gaitera <gaiterajomariel@gmail.com>"
  ]
}
