<div class="p-5 overflow-hidden shadow-md rounded-2xl items-center inline-flex bg-gray-500/10">
    <img
      class="rounded-full w-12 h-12"
      src="<%= user.picture %>"
      alt="User profile picture"
      crossorigin="anonymous"
    />
    <div class="m-2 flex gap-x-2">
      <h3 class=" font-poppins text-lg">
        <%= user.UserRole.role %>
      </h3>
      <p><%= user.displayName %></p>
      <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6 pl-1">
        <path stroke-linecap="round" stroke-linejoin="round" d="M19.5 8.25l-7.5 7.5-7.5-7.5" />
      </svg>
      
    </div>
  </div>